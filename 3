items = [
    [34587, 'Mark Lutz', 4, 40.95],
    [98762, 'Mark Lutz 2', 5, 56.60],
    [77226, 'Paul Berry', 3, 32.95],
    [88112, 'Bernd Klien', 3, 24.99]
]

print(list(map(lambda item: (item[0], (item[-1] * item[-2]) + 10 if item[-1] * item[-2] < 100 else item[-1] * item[-2]),
               items)))

